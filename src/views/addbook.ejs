<div class="container col-6 p-5" id ='myform' >

          <!--/stories-->
            <div class="row">    
              <form method="POST" action="/addBook/add" class="form">
                <div class="mb-3 form-control">
                  <label for="Title">Book Name</label>
                  <input type="text" placeholder="Enter Book Name" id="title" autocomplete="off" name="title"/>
                </div>
                <br>
                <div class="mb-3 form-control">
                  <label for="Author">Author</label>
                  <input type="text" placeholder="Enter Author Name" id="author" autocomplete="off" name="author"/>
                </div>
                <br>

                <div class="mb-3 form-control">
                  <label for="Genre">Genre</label>
                  <input type="text" placeholder="Enter Book Genre" id="genre" autocomplete="off" name="genre"/>
                </div>
                <br>
                <div class="mb-3 form-control">
                  <label for="Image">Image</label>
                  <input type="file" placeholder="Enter Book Image" id="image" name="image"/>
                </div>
                <br>
                <br>
                <button type="submit">Add Book</button>
                
              </form>
            </div>

